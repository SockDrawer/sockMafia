<u>Votecount for Day {{day}}</u>
  <table>
  {{#each votes}}
  <tr>
  <td>{{voteChart percent}}</td>
  <td><b>{{target}} ({{num}})</b>:</td>
  <td>{{#each names}}<b>{{#if retracted}}<s>{{/if}}{{voter}}{{#if retracted}}</s>{{/if}},{{/each}}
  </tr>
  {{/each}}
  <tr>
  <td></td>
  <td>Not Voting ({{numNotVoting}}):</td>
  <td>{{#each notVoting}}{{this}},{{/each}}</td>
  </tr>
  </table>